help = show tiup topology yaml file by record-id(s)
abbr = topology|tiup|yaml
quiet-error = true

[arg]
bench-result-record-id|record-id|rid|id = ''
to-file = ''

[arg2env]
bench.result.filter.record-id = bench-result-record-id

[env]
bench.meta.host = read
bench.meta.port = read
bench.meta.db-name = read
bench.meta.user = read
bench.result.filter.record-id = read

[dep]
base64 = to decode topology file from text
