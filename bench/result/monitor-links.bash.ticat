help = show grafana links related to a bench record id
abbr = monitor-link|monitor|mon|grafana
quiet-error = true

[arg]
bench-result-record-id|record-id|rid|id = ''
host-or-ip|host|ip|h = ''

[arg2env]
bench.result.filter.record-id = bench-result-record-id

[env]
bench.meta.host = read
bench.meta.port = read
bench.meta.db-name = read
bench.meta.user = read
