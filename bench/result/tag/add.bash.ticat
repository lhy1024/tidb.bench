help = add tag or tag list to a bench record or records

[arg]
tags|tag|t = ''
bench-result-record-id-list|record-ids|record-id|rid|ids = ''

[arg2env]
bench.meta.result.ids = bench-result-record-id-list

[env]
bench.meta.host = read
bench.meta.port = read
bench.meta.db-name = read
bench.meta.user = read
bench.meta.result.ids = read
