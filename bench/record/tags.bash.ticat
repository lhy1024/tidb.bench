help = record `bench.tag` and all `bench.tag.*` in env to meta db as tags of current run

[arg]
user-tags|tags|tag = ''

[arg2env]
bench.tag = user-tags

[env]
bench.meta.host = read
bench.meta.port = read
bench.meta.user = read
bench.meta.db-name = read
bench.workload = read
bench.tag = read

[dep]
mysql = as client, to write record data
