help = select bench result by record-id(s), will show in the coming bench.result
abbr = by-record|record|rec|rid
quiet-err = true

[arg]
record-id-list|record-ids|record-id|rid|record-list|records|record|rec|r|id = ''
as-baseline|baseline|base = false
max-records|max-cnt|max|limit = 0

[env]
bench.meta.host = read
bench.meta.port = read
bench.meta.db-name = read
bench.meta.user = read

bench.meta.result.ids = write
