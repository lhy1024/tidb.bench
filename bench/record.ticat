
help = record current bench result to meta db
abbr = rec
trivial = 3

flow = bench.record.jitter+usage : [[bench.workload]].record

[arg]
bench-record-tag|tags|tag = ''
cluster-name|cluster = ''
workload|wl = ''
bench-record-begin|record-begin|begin = ''
bench-record-end|record-end|end = ''

[arg2env]
