help = clear all bench results
abbr = clean|clr

[arg]
mysql-host|host|h = 127.0.0.1
mysql-port|port|p = 4100
mysql-user|user|u = root
mysql-db-name|db-name|db = bench_meta

[arg2env]
bench.meta.host = mysql-host
bench.meta.port = mysql-port
bench.meta.user = mysql-user
bench.meta.db-name = mysql-db-name

[env]
bench.meta.host = read
bench.meta.port = read
bench.meta.user = read
bench.meta.db-name = read

[dep]
mysql = as client
